{
    "manifest_version": 2,
    "name": "YouTube Scheduler",
    "version": "1.0",
    "description": "Schedule YouTube video notifications",
    "permissions": ["storage", "activeTab", "notifications"],
    "browser_action": {
      "default_icon": {
        "16": "16.png",
        "48": "48.png",
        "128": "128.png"
      },
      "default_title": "YouTube Scheduler",
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["*://www.youtube.com/*"],
        "js": ["content.js"]
      }
    ],
    "web_accessible_resources": [
      "bootstrap-icons-1.11.3/font/bootstrap-icons.css",
      "bootstrap-icons-1.11.3/font/fonts/bootstrap-icons.woff",
      "bootstrap-icons-1.11.3/font/fonts/bootstrap-icons.woff2"
    ]
  }
  